                Box1.Text = Label6.Text And Label6.Text = Label11.Text And Label11.Text = Label16.Text And Label16.Text = Label21.Text And Box1.Text <> " " Or
         Label2.Text = Label7.Text And Label7.Text = Label12.Text And Label12.Text = Label17.Text And Label17.Text = Label22.Text And Label22.Text <> " " Or
      Label3.Text = Label8.Text And Label8.Text = Label13.Text And Label13.Text = Label18.Text And Label18.Text = Label23.Text And Label3.Text <> " " Or
