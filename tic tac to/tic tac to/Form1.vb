            Label5.Text = Label10.Text And Label10.Text = Label15.Text And Label15.Text = Label20.Text And Label20.Text = Label25.Text And Label5.Text <> " " Or
               Box1.Text = Label7.Text And Label7.Text = Label13.Text And Label13.Text = Label19.Text And Label19.Text = Label25.Text And Box1.Text <> " " Or
                  Label21.Text = Label17.Text And Label17.Text = Label13.Text And Label13.Text = Label9.Text And Label9.Text = Label5.Text And Label21.Text <> " " Then



            gameover = True
